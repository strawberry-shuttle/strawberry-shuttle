#include <highgui.h>
#include <cv.h>
#include <cxcore.h>
#include <math.h>
#include <vector>
#include <stdio.h>

#include <iostream>

using namespace cv;

using namespace std;

int main(int argc, char* argv[])
{
	IplImage* image = 0;
	if( (image = cvLoadImage(argv[3])) == 0)
	{
		cout<<"Could not load image "<<argv[3]<<endl;
		return -1;
	}
	int imageWidth = image->width;
	int imageHeight = image->height;
	cout<<"height"<<imageHeight<<"width"<<imageWidth<<endl;
}
